  <rdf:Description rdf:about="http://onama.sbg.ac.at/PatientRole">
    <rdfs:label xml:lang="de">Rolle Patiens</rdfs:label>
    <dc:description xml:lang="en">Patient - The living entity affected by an event or action and not exercising control over the event; the object or matter affected.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/UndergoerRole"/>
    <rdfs:label xml:lang="en">Patient Role</rdfs:label>
    <dc:description xml:lang="de">Patiens - Die von einem Geschehen betroffene lebende Entität, die keine Kontrolle auf den Vorgang ausübt; die betroffene Sache oder der betroffene Sachverhalt.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TextualRealisation">
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Konzepts in einem Text.</dc:description>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/VisualRealisation"/>
    <dc:description xml:lang="en">Denotes the concrete implementation of a concept in a text.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="en">Textual Realisation</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Textuelle Realisierung</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ParticipatorRole">
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ActantRole"/>
    <rdfs:label xml:lang="en">Participator Role</rdfs:label>
    <dc:description xml:lang="de">Die Klasse umfasst typischerweise belebte Entitäten, die am Geschehen teilhaben, aber keine Kontrolle oder Initiative im Zusammenhang damit aufweisen.</dc:description>
    <dc:description xml:lang="en">The class includes typicallly animate entities which participate in the event but have no control or initiative associated it.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Teilnehmende/r</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/RecipientRole">
    <rdfs:label xml:lang="en">Recipient Role</rdfs:label>
    <dc:description xml:lang="de">Der Empfänger einer Entität oder Information bei Verben, die den Transfer eines Besitzgegenstandes oder einer Information bezeichnen.</dc:description>
    <rdfs:label xml:lang="de">Rolle Rezipient</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ParticipatorRole"/>
    <dc:description xml:lang="en">The recipient of an entity or information in verbs that denote the transfer of an object of possession or information.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TemporalityRole">
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <dc:description xml:lang="de">Gibt den Zeitraum oder Zeitpunkt eines Geschehens an.</dc:description>
    <rdfs:label xml:lang="en">Temporality Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Zeitlichkeit</rdfs:label>
    <dc:description xml:lang="en">Indicates the period or time of an event.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/PersistentItem">
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/TimeSpan"/>
    <dc:description xml:lang="de">Diese Klasse umfasst Lebewesen, Gegenstände und geistige Erzeugnisse, die eine dauerhafte Identität haben.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
