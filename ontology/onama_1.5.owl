    <!-- http://onama.sbg.ac.at/ontology#broader -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#broader">
        <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/ontology#broaderTransitive"/>
        <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/ontology#narrower"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <dc:description xml:lang="de">Die Kante &quot;umfassender&quot; verweist von einem Konzept eines Narraivs auf das unmittelbar übergeordnete Konzept und zeigt damit an, dass ein Narrativ Teil eines anderen Narrativs ist.</dc:description>
        <dc:description xml:lang="en">The property &quot;broader&quot; relates a narrative concept to the immediately superordinate concept, indicating that a narrative is part of another narrative.</dc:description>
        <rdfs:label xml:lang="en">broader</rdfs:label>
        <rdfs:label xml:lang="de">umfassender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#broaderTransitive -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#broaderTransitive">
        <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/ontology#isRelatedTo"/>
        <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/ontology#narrowerTransitive"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <dc:description xml:lang="de">Diese Kante verbindet ein Konzept eines Narrativs mit sämtlichen übergeordneten Konzepten.</dc:description>
        <dc:description xml:lang="en">This property relates a narrative concept to all the hierarchically superordinated narrative concepts.</dc:description>
        <rdfs:label xml:lang="en">broader transitive</rdfs:label>
        <rdfs:label xml:lang="de">transitiv umfassender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#firstInCollection -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#firstInCollection">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Collection"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Realisation"/>
        <dc:description xml:lang="de">Diese Kante verbindet eine Sammlung mit dem ersten Narrativ (Realisierung) in der Sammlung.</dc:description>
        <dc:description xml:lang="en">This property relates a collection with the first narrative (realisation) in the collection.</dc:description>
        <rdfs:label xml:lang="de">erstes Element in einer Sammlung</rdfs:label>
        <rdfs:label xml:lang="en">first in collection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasAction -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#hasAction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Narrative"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Action"/>
        <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ mit der Handlung, die im Narrativ stattfindet.</dc:description>
        <dc:description xml:lang="en">This property relates a narrative to the action taking place in the narrative.</dc:description>
        <rdfs:label xml:lang="en">has action</rdfs:label>
        <rdfs:label xml:lang="de">hat Handlung</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasActor -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#hasActor">
