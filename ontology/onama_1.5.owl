        <rdfs:label xml:lang="de">hat WikiData Äquivalent</rdfs:label>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Collection"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Narrative"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#PersistentItem"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Place"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#TemporalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#isRealisedAsSupplementaryNarrativeBy -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#isRealisedAsSupplementaryNarrativeBy">
        <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/ontology#isRealisedBy"/>
        <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/ontology#realisesAsSupplementaryNarrative"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Realisation"/>
        <dc:description xml:lang="de">Diese Eigenschaft verbindet jene Konzepte, die eine &quot;Haupthandlung&quot; ergänzen, mit ihren Realisierungen.</dc:description>
        <dc:description xml:lang="en">This property relates those concepts that are supplementary to a &quot;main plot&quot; to their realisations.</dc:description>
        <rdfs:label xml:lang="en">is realised as supplementary narrative by</rdfs:label>
        <rdfs:label xml:lang="de">ist realisiert als ergänzendes Narrativ durch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#isRealisedBy -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#isRealisedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/ontology#realises"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Concept"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#Realisation"/>
        <dc:description xml:lang="de">Das Narrativ ist realisiert durch ein spezifisches Werk der Literatur oder der bildenden Kunst; diese Kante verbindet das Konzept eines Narrativs mit dessen Realisierung.</dc:description>
        <dc:description xml:lang="en">The narrative is realized through a specific work of literature or the visual arts; this property relates the concept of a narrative to its realisation.</dc:description>
        <rdfs:label xml:lang="en">is realised by</rdfs:label>
        <rdfs:label xml:lang="de">ist realisiert durch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#isRelatedTo -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#isRelatedTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
