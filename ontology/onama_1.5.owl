        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dcterms:modified>
        <dcterms:title xml:lang="de">Ontologie der Narrative des Mittelalters</dcterms:title>
        <dcterms:title xml:lang="en">Ontology of Narratives of the Middle Ages</dcterms:title>
