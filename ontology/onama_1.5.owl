    <owl:DatatypeProperty rdf:about="http://onama.sbg.ac.at/ontology#hasSceneNode">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Action"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <dc:description xml:lang="en">Reference to a node of a REALonline scene graph.</dc:description>
        <dc:description xml:lang="de">Referenz auf einen Knoten eines REALonline-Szenegraphen.</dc:description>
        <rdfs:label xml:lang="en">has REALonline scenary graph node</rdfs:label>
        <rdfs:label xml:lang="en">has scenary graph node</rdfs:label>
        <rdfs:label xml:lang="de">hat REALonline Szenengraphknoten</rdfs:label>
        <rdfs:label xml:lang="de">hat Szenengraphknoten</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasSigil -->

    <owl:DatatypeProperty rdf:about="http://onama.sbg.ac.at/ontology#hasSigil">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#Collection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dc:description xml:lang="de">Diese Kante verbindet eine Sammlung mit einer Sigle in der MHDBDB.</dc:description>
        <dc:description xml:lang="en">This property relates a Collection to a sigil in the MHDBDB.</dc:description>
        <rdfs:label xml:lang="en">has MHDBDB sigil</rdfs:label>
        <rdfs:label xml:lang="en">has sigil</rdfs:label>
        <rdfs:label xml:lang="de">hat MHDBDB Sigle</rdfs:label>
        <rdfs:label xml:lang="de">hat Sigle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasUrl -->

    <owl:DatatypeProperty rdf:about="http://onama.sbg.ac.at/ontology#hasUrl">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <dc:description xml:lang="en">Reference to an URL.</dc:description>
        <dc:description xml:lang="de">Verweis auf eine URL.</dc:description>
        <rdfs:label xml:lang="en">has Url</rdfs:label>
        <rdfs:label xml:lang="de">hat Url</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#toToken -->

    <owl:DatatypeProperty rdf:about="http://onama.sbg.ac.at/ontology#toToken">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#TextPassage"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dc:description xml:lang="de">Diese Kante zeigt von einer Textpassage auf das letzte Token aus dieser Gruppe.</dc:description>
        <dc:description xml:lang="en">This property points from a Text Passage to the last token of this group.</dc:description>
        <rdfs:label xml:lang="de">bis Token</rdfs:label>
        <rdfs:label xml:lang="en">to token</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#closeMatch -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    </owl:DatatypeProperty>
    


