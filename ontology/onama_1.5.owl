        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#TemporalEntity"/>
        <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit einer zeitlichen Entität, die diese Rolle besetzt.</dc:description>
        <dc:description xml:lang="en">This property relates a semantic role to a temporal entity that holds this role.</dc:description>
        <rdfs:label xml:lang="en">has temporal entity</rdfs:label>
        <rdfs:label xml:lang="de">hat zeitliche Entität (Geschehendes)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasTemporaryEntityFunction -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#hasTemporaryEntityFunction">
        <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/ontology#hasEntityFunction"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#EntityFunction"/>
        <dc:description xml:lang="de">Diese Kante verbindet einen Knoten vom Typ &quot;Semantische Rolle&quot; mit einer Entitätsfunktion. Die Funktion trifft also zeitweilig - für das Narrativ - auf den Rolleninhaber (Akteur oder Ding) zu.</dc:description>
        <dc:description xml:lang="en">This property relates a node of type &quot;Semantic Role&quot; to an entity function. The function thus applies temporarily - for the narrative - to the actor or thing that holds the role.</dc:description>
        <rdfs:label xml:lang="en">has temporary entity function</rdfs:label>
        <rdfs:label xml:lang="de">hat zeitweilige Entitätsfunktion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://onama.sbg.ac.at/ontology#hasTextPassage -->

    <owl:ObjectProperty rdf:about="http://onama.sbg.ac.at/ontology#hasTextPassage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://onama.sbg.ac.at/ontology#TextualRealisation"/>
        <rdfs:range rdf:resource="http://onama.sbg.ac.at/ontology#TextPassage"/>
