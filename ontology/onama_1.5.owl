    <rdf:Description rdf:about="http://erlangen-crm.org/current/E21_Person">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#Person"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E2_Temporal_Entity -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E2_Temporal_Entity">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#TemporalEntity"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E39_Actor -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E39_Actor">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#Actor"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E52_Time-Span -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E52_Time-Span">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#TimeSpan"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E53_Place -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E53_Place">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#Place"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E70_Thing -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E70_Thing">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#Thing"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E74_Group -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E74_Group">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#Group"/>
    </rdf:Description>
    


    <!-- http://erlangen-crm.org/current/E77_Persistent_Item -->

    <rdf:Description rdf:about="http://erlangen-crm.org/current/E77_Persistent_Item">
        <owl:equivalentClass rdf:resource="http://onama.sbg.ac.at/ontology#PersistentItem"/>
    </rdf:Description>
    


    <!-- http://onama.sbg.ac.at/ontology#ActantRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#ActantRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <dc:description xml:lang="en">Actants are elements that perform or are subjected to an action.</dc:description>
        <dc:description xml:lang="de">Aktanten sind Elemente, die eine Handlung ausführen oder von einer Handlung betroffen sind.</dc:description>
        <rdfs:label xml:lang="en">Actant Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Aktant</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Action -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Action">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E28_Conceptual_Object"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <dc:description xml:lang="en">Abstract concepts of actions from the class &quot;Verb&quot; are created as a separate node for each narrative, which belongs to the class &quot;Action&quot;.</dc:description>
        <dc:description xml:lang="de">Abstrakte Handlungskonzepte aus der Klasse &quot;Verb&quot; werden für jedes Narrativ als eigener Knoten angelegt, welcher der Klasse &quot;Handlung&quot; angehört.</dc:description>
        <rdfs:label xml:lang="en">Action</rdfs:label>
        <rdfs:label xml:lang="de">Handlung</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Actor -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Actor">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#PersistentItem"/>
        <dc:description xml:lang="de">Bezeichnet Protagonisten, die in einem Narrativ aktiv werden.</dc:description>
        <dc:description xml:lang="en">Denotes protagonists who become active in a narrative.</dc:description>
        <rdfs:label xml:lang="en">Actor</rdfs:label>
        <rdfs:label xml:lang="de">Akteur</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#AgentRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#AgentRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#CauserRole"/>
        <dc:description xml:lang="en">A prototypical agent is a living entity who acts on purpose and performs an action that has a physical, visible effect.</dc:description>
        <dc:description xml:lang="de">Ein prototypisches Agens ist eine belebte Entität, die absichtlich handelt und deren Handlung einen physischen, sichtbaren Effekt hat.</dc:description>
        <rdfs:label xml:lang="en">Agent Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Agens</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Animal -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Animal">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#Actor"/>
        <hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E20"/>
        <dc:description xml:lang="de">Bezeichnet ein Tier als Protagonisten, welches in einem Narrativ involviert ist.</dc:description>
        <dc:description xml:lang="en">Refers to an animal as a protagonist that is involved in a narrative.</dc:description>
        <rdfs:label xml:lang="en">Animal</rdfs:label>
        <rdfs:label xml:lang="de">Tier</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#BenefactiveRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#BenefactiveRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#ParticipatorRole"/>
        <dc:description xml:lang="en">Benefactive is the semantic role of a referent which is advantaged or disadvantaged by an event.</dc:description>
        <dc:description xml:lang="de">Benefaktiv ist die semantische Rolle eines Referenten, der durch ein Ereignis begünstigt oder benachteiligt wird.</dc:description>
        <rdfs:label xml:lang="en">Benefactive Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Benefaktiv</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#CauserRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#CauserRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#ActantRole"/>
        <dc:description xml:lang="en">Causer is the semantic role of a person or thing who is the doer or the instigator of an event.</dc:description>
        <dc:description xml:lang="de">Verursacher/in ist die semantische Rolle einer Person oder Sache, die eine Handlung ausführt bzw. ein Ereignis auslöst.</dc:description>
        <rdfs:label xml:lang="en">Causer Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Verursacher/in</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Co-AgentRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Co-AgentRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#CauserRole"/>
        <dc:description xml:lang="en">Co- or Re-Agent in verbs that describe actions that are in the relation of action and reaction to each other.</dc:description>
        <dc:description xml:lang="de">Ko- oder Re-Agierender bei Verben, die Handlungen beschreiben, die in der Relation von Aktion und Reaktion zueinander stehen.</dc:description>
        <rdfs:label xml:lang="en">Co-Agent Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Ko-Agens</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Collection -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Collection">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E28_Conceptual_Object"/>
        <dc:description xml:lang="de">Benennt den textlichen oder bildlichen Kontext, aus welchem das Narrativ stammt.</dc:description>
        <dc:description xml:lang="en">Identifies the textual or pictorial context from which the narrative originates.</dc:description>
        <rdfs:label xml:lang="en">Collection</rdfs:label>
        <rdfs:label xml:lang="de">Sammlung</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Concept -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Concept">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#Narrative"/>
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#Realisation"/>
        <dc:description xml:lang="en">Defines a specific narrative as an abstraction without a concrete reference to a realization.</dc:description>
        <dc:description xml:lang="de">Definiert ein spezifisches Narrativ als Abstraktum ohne einen konkreten Rückbezug auf eine Realisierung.</dc:description>
        <rdfs:label xml:lang="en">Concept</rdfs:label>
        <rdfs:label xml:lang="de">Konzept</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#EntityFunction -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#EntityFunction">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E28_Conceptual_Object"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <dc:description xml:lang="de">Bezeichnet eine Funktion eines Akteurs oder  Dings in einem Narrativ (beispielsweise Held, Herrscher, Bösewicht, Trophäe, Talisman...).</dc:description>
        <dc:description xml:lang="en">Denotes a function of an actor or thing in a narrative (for example hero, ruler, villain, trophy, talisman...).</dc:description>
        <rdfs:label xml:lang="en">Entity Function</rdfs:label>
        <rdfs:label xml:lang="de">Entitätsfunktion</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#ExperiencerRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#ExperiencerRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#ParticipatorRole"/>
        <dc:description xml:lang="en">An Experiencer is an animate entity that experiences a sensory perception or mental state designated by the predicate.</dc:description>
        <dc:description xml:lang="de">Ein/e Wahrnehmende/r ist eine belebte Entität, die eine vom Prädiktat bezeichnete Sinneswahrnehmung oder einen psychischen Zustand erlebt.</dc:description>
        <rdfs:label xml:lang="en">Experiencer Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Wahrnehmende/r</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#GoalRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#GoalRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#LocalisationRole"/>
        <dc:description xml:lang="en">Goal – The location or entity in the direction of which something moves.</dc:description>
        <dc:description xml:lang="de">Ziel - Der Ort oder die Entität, in deren Richtung sich etwas bewegt.</dc:description>
        <rdfs:label xml:lang="en">Goal Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Ziel</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Group -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Group">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#Actor"/>
        <hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
        <dc:description xml:lang="de">Bezeichnet eine Gruppe von Individuen, die in einem Narrativ involviert sind.</dc:description>
        <dc:description xml:lang="en">Denotes a group of individuals who are involved in a narrative.</dc:description>
        <rdfs:label xml:lang="en">Group</rdfs:label>
        <rdfs:label xml:lang="de">Menschliche Gruppe</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#InstrumentRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#InstrumentRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <dc:description xml:lang="de">Instrument - Das Mittel, mit dem die durch das Prädikat bezeichnete Handlung oder das Ereignis ausgeführt wird.</dc:description>
        <dc:description xml:lang="en">Instrument – The medium by which the action or event denoted by the predicate is carried out.</dc:description>
        <rdfs:label xml:lang="en">Instrument Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Instrument</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#LocalisationRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#LocalisationRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <dc:description xml:lang="en">Local Averbial; Adverbial of Place.</dc:description>
        <dc:description xml:lang="de">Lokaladverbiale; Adverbiale des Raumes.</dc:description>
        <rdfs:label xml:lang="en">Localisation Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Lokalisierung</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#ModeRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#ModeRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <dc:description xml:lang="en">&quot;Adverbial of manner – procedure (how?); degree, measure, intensity (how much?); material quality (from what?)</dc:description>
        <dc:description xml:lang="de">Adverbiale der Art und Weise – Vorgehensweise (wie?); Grad, Maß, Intensität (wieviel, wie sehr...?); stoffliche Beschaffenheit (woraus?)</dc:description>
        <rdfs:label xml:lang="en">Mode Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Modus</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#MythicalBeing -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#MythicalBeing">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#Actor"/>
        <hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E28"/>
        <dc:description xml:lang="de">Bezeichnet ein mythisches Wesen, eine Gottheit oder ein Fabelwesen, welches in einem Narrativ involviert ist.</dc:description>
        <dc:description xml:lang="en">Denotes a mythical being or a deity that is involved in a narrative.</dc:description>
        <rdfs:label xml:lang="en">MythicalBeing</rdfs:label>
        <rdfs:label xml:lang="de">Mythisches Wesen; Gottheit; Fabelwesen</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Narrative -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Narrative">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E28_Conceptual_Object"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E5"/>
        <dc:description xml:lang="de">Bezeichnet eine Handlung, eine Zustandsänderung.</dc:description>
        <dc:description xml:lang="en">Denotes an action, a change of state.</dc:description>
        <rdfs:label xml:lang="de">Narrativ</rdfs:label>
        <rdfs:label xml:lang="en">Narrative</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#ParticipatorRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#ParticipatorRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#ActantRole"/>
        <dc:description xml:lang="de">Die Klasse umfasst typischerweise belebte Entitäten, die am Geschehen teilhaben, aber keine Kontrolle oder Initiative im Zusammenhang damit aufweisen.</dc:description>
        <dc:description xml:lang="en">The class includes typicallly animate entities which participate in the event but have no control or initiative associated it.</dc:description>
        <rdfs:label xml:lang="en">Participator Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Teilnehmende/r</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#PatientRole -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#PatientRole">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#UndergoerRole"/>
        <dc:description xml:lang="de">Patiens - Die von einem Geschehen betroffene lebende Entität, die keine Kontrolle auf den Vorgang ausübt; die betroffene Sache oder der betroffene Sachverhalt.</dc:description>
        <dc:description xml:lang="en">Patient - The living entity affected by an event or action and not exercising control over the event; the object or matter affected.</dc:description>
        <rdfs:label xml:lang="en">Patient Role</rdfs:label>
        <rdfs:label xml:lang="de">Rolle Patiens</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#PersistentItem -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#PersistentItem">
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#Place"/>
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#SemanticRole"/>
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#TimeSpan"/>
        <hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E77_Persistent_Item"/>
        <dc:description xml:lang="de">Diese Klasse umfasst Lebewesen, Gegenstände und geistige Erzeugnisse, die eine dauerhafte Identität haben.</dc:description>
        <dc:description xml:lang="en">This class includes living beings, objects and intellectual products which have a permanent identity.</dc:description>
        <rdfs:label xml:lang="en">Persistent Item</rdfs:label>
        <rdfs:label xml:lang="de">Persistentes Objekt (Seiendes)</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Person -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Person">
        <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ontology#Actor"/>
        <hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
        <dc:description xml:lang="de">Bezeichnet eine Person, welche in einem Narrativ involviert ist.</dc:description>
        <dc:description xml:lang="en">Denotes a person who is involved in a narrative.</dc:description>
        <rdfs:label xml:lang="de">Person</rdfs:label>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </owl:Class>
    


    <!-- http://onama.sbg.ac.at/ontology#Place -->

    <owl:Class rdf:about="http://onama.sbg.ac.at/ontology#Place">
        <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/ontology#TimeSpan"/>
        <hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <dc:description xml:lang="de">Bezeichnet Ausdehnungen im Raum, die in der Regel unbeweglich sind (beispielsweise Burg, Garten, Tal...).</dc:description>
        <dc:description xml:lang="en">Describes extensions in space that are usually immobile (for example castle, garden, valley...).</dc:description>
        <rdfs:label xml:lang="de">Ort</rdfs:label>
        <rdfs:label xml:lang="en">Place</rdfs:label>
    </owl:Class>
    


