        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#sameAs"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Collection"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Narrative"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#PersistentItem"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#Place"/>
                    <rdf:Description rdf:about="http://onama.sbg.ac.at/ontology#TemporalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <dc:description xml:lang="de">Diese Eigenschaft/dieses Narrativ hat eine GND Entsprechung.</dc:description>
        <dc:description xml:lang="en">This property/narrative has a GND equivalent.</dc:description>
        <rdfs:label xml:lang="en">has GND equivalent</rdfs:label>
        <rdfs:label xml:lang="de">hat GND Äquivalent</rdfs:label>
    </owl:ObjectProperty>
    


