    <rdfs:label xml:lang="de">Textpassage</rdfs:label>
    <dc:description xml:lang="en">Denotes a section from a text; for instance in the Mittelhochdeutsche Begriffsdatenbank (MHDBDB).</dc:description>
    <dc:description xml:lang="de">Bezeichnet einen Ausschnitt aus einem Text; beispielsweise in der Mittelhochdeutschen Begriffsdatenbank (MHDBDB).</dc:description>
    <rdfs:label xml:lang="en">Text Passage</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N3c4bf7f54a4940cc99e13a6909f4105d">
    <rdf:rest rdf:nodeID="N033d6ca63e8f4ff5b5c7601d41579aac"/>
    <rdf:first rdf:resource="http://onama.sbg.ac.at/Narrative"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/SourceRole">
    <rdfs:label xml:lang="de">Rolle Quelle</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Source – The location or entity from which something moves.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/LocalisationRole"/>
    <rdfs:label xml:lang="en">Source Role</rdfs:label>
    <dc:description xml:lang="de">Quelle - Der Ort oder die Entität, von der aus sich etwas bewegt.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/firstInCollection">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="en">first in collection</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="en">This property relates a collection with the first narrative (realisation) in the collection.</dc:description>
    <rdfs:label xml:lang="de">erstes Element in einer Sammlung</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet eine Sammlung mit dem ersten Narrativ (Realisierung) in der Sammlung.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Collection"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:label xml:lang="en">label</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N05df41f733ac456eadfcb9d3fbc61bd4">
    <rdf:first rdf:resource="http://onama.sbg.ac.at/Place"/>
    <rdf:rest rdf:nodeID="N56a7228ba83345cea8b6b112cdb457df"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/InstrumentRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Instrument - Das Mittel, mit dem die durch das Prädikat bezeichnete Handlung oder das Ereignis ausgeführt wird.</dc:description>
    <rdfs:label xml:lang="en">Instrument Role</rdfs:label>
    <dc:description xml:lang="en">Instrument – The medium by which the action or event denoted by the predicate is carried out.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdfs:label xml:lang="de">Rolle Instrument</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/next">
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="en">next</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das nächste Narrativ.</dc:description>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the next narrative.</dc:description>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/previous"/>
    <rdfs:label xml:lang="de">nächstes</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/isRelatedTo"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Actor">
    <rdfs:label xml:lang="en">Actor</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet Protagonisten, die in einem Narrativ aktiv werden.</dc:description>
    <rdfs:label xml:lang="de">Akteur</rdfs:label>
    <dc:description xml:lang="en">Denotes protagonists who become active in a narrative.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/PersistentItem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasSupplementaryConcept">
    <rdfs:label xml:lang="en">has supplementary concept</rdfs:label>
    <rdfs:label xml:lang="de">hat ergänzendes Konzept</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ-Konzept mit einem ergänzenden Narrativ-Konzept. Ein ergänzendes Konzept ist ein Narrativ, das zusätzlich zur jeweiligen "Haupthandlung" geschildert wird.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/isRelatedTo"/>
    <dc:description xml:lang="en">This property relates a narrative concept to a supplementary narrative concept. A supplementary concept is a narrative, which is presented in addition to the respective "main action".</dc:description>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasThing">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="en">This property relates a semantic role to an intellectual product or physical thing that holds this role.</dc:description>
    <rdfs:label xml:lang="de">hat Ding</rdfs:label>
    <rdfs:label xml:lang="en">has thing</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet eine sematische Rolle mit einem intellektuellen Produkt oder physischen Ding, das diese Rolle besetzt.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Thing"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Ne3bf5b9672ef4ac7a869e61cc89a0120">
    <rdf:rest rdf:nodeID="Ncac387a642674a659144f32e07cea8c9"/>
    <rdf:first rdf:resource="http://onama.sbg.ac.at/TemporalEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/AgentRole">
    <rdfs:label xml:lang="en">Agent Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Ein prototypisches Agens ist eine belebte Entität, die absichtlich handelt und deren Handlung einen physischen, sichtbaren Effekt hat.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/CauserRole"/>
    <dc:description xml:lang="en">A prototypical agent is a living entity who acts on purpose and performs an action that has a physical, visible effect.</dc:description>
    <rdfs:label xml:lang="de">Rolle Agens</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasGeneralEntityFunction">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/PersistentItem"/>
    <rdfs:label xml:lang="de">hat allgemeine Entitätsfunktion</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet einen Akteur oder ein Ding mit einer allgemein zutreffenden Entitätsfunktion.</dc:description>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/EntityFunction"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/hasEntityFunction"/>
    <dc:description xml:lang="en">This property relates an actor or a thing to a generally applicable entity function.</dc:description>
    <rdfs:label xml:lang="en">has general entity function</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/previousFabula">
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the previous narrative on the fabula path.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#P120i"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/previous"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ auf dem Fabula-Pfad.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">vorhergehende Fabula</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="en">previous fabula</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasMotifIndexEquivalent">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#sameAs"/>
    <rdfs:label xml:lang="de">hat Motiv-Index Äquivalent</rdfs:label>
    <dc:description xml:lang="de">Dieses Narrativ hat eine Entsprechung im Motif-Index.</dc:description>
    <rdfs:label xml:lang="en">has Motif-Index equivalent</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Narrative"/>
    <dc:description xml:lang="en">This narrative has an Motif Index equivalent.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/nextFabula">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="en">next fabula</rdfs:label>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/previousFabula"/>
    <rdfs:label xml:lang="de">nächste Fabula</rdfs:label>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the next narrative on the fabula path.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/next"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das nächste Narrativ auf dem Fabula-Pfad.</dc:description>
    <onama:hasCidocEquivalent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#P120</onama:hasCidocEquivalent>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/CauserRole">
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ActantRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Causer is the semantic role of a person or thing who is the doer or the instigator of an event.</dc:description>
    <rdfs:label xml:lang="en">Causer Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Verursacher</rdfs:label>
    <dc:description xml:lang="de">Verursacher ist die semantische Rolle einer Person oder Sache, die eine Handlung ausführt bzw. ein Ereignis auslöst.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TextualRealisation">
    <dc:description xml:lang="en">Denotes the concrete implementation of a concept in a text.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Textuelle Realisierung</rdfs:label>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/VisualRealisation"/>
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Konzepts in einem Text.</dc:description>
    <rdfs:label xml:lang="en">Textual Realisation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/previousSyuzhet">
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="de">vorhergehendes Syuzhet</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ auf dem Syuzhet-Pfad.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/previous"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the prevoious narrative on the syuzhet path.</dc:description>
    <rdfs:label xml:lang="en">previous syuzhet</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasSceneNode">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <dc:description xml:lang="en">Reference to a node of a REALonline scene graph.</dc:description>
    <rdfs:label xml:lang="en">has scenary graph node</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has REALonline scenary graph node</rdfs:label>
    <rdfs:label xml:lang="de">hat REALonline Szenengraphknoten</rdfs:label>
    <dc:description xml:lang="de">Referenz auf einen Knoten eines REALonline-Szenegraphen.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Action"/>
    <rdfs:label xml:lang="de">hat Szenengraphknoten</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasMhdbdbSigilURL">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/hasUrl"/>
    <dc:description xml:lang="en">Reference to the URL of a MHDBDB sigil.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Collection"/>
    <rdfs:label xml:lang="de">hat URL auf MHDBDB Sigle</rdfs:label>
    <dc:description xml:lang="de">Verweis auf eine die URL zu einer MHDBDB Sigle.</dc:description>
    <rdfs:label xml:lang="en">has Mhdbdb Sigil URL</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/broader">
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:label xml:lang="en">broader</rdfs:label>
    <dc:description xml:lang="en">The property "broader" relates a narrative concept to the immediately superordinate concept, indicating that a narrative is part of another narrative.</dc:description>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/narrower"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <dc:description xml:lang="de">Die Kante "umfassender" verweist von einem Konzept eines Narraivs auf das unmittelbar übergeordnete Konzept und zeigt damit an, dass ein Narrativ Teil eines anderen Narrativs ist.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/broaderTransitive"/>
    <rdfs:label xml:lang="de">umfassender</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasActor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Actor"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="en">has actor</rdfs:label>
    <dc:description xml:lang="en">This property relates a semantic role to the actor who holds this role.</dc:description>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit dem Akteur, der diese Rolle besetzt.</dc:description>
    <rdfs:label xml:lang="de">hat Akteur</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasEntityFunction">
    <dc:description xml:lang="en">This property relates an actor or a thing to an Entity Function. Please use one of the two subproperties.</dc:description>
    <rdfs:label xml:lang="en">has Entity Function</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">hat Entitätsfunktion</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet einen Akteur, ein Ding oder eine Semantische Rolle mit einer Entitätsfunktion. Bitte verwenden Sie eine der beiden Subproperties.</dc:description>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/EntityFunction"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasTemporaryEntityFunction">
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/EntityFunction"/>
    <rdfs:label xml:lang="de">hat zeitweilige Entitätsfunktion</rdfs:label>
    <rdfs:label xml:lang="en">has temporary entity function</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/hasEntityFunction"/>
    <dc:description xml:lang="en">This property relates a node of type "Semantic Role" to an entity function. The function thus applies temporarily - for the narrative - to the actor or thing that holds the role.</dc:description>
    <dc:description xml:lang="de">Diese Kante verbindet einen Knoten vom Typ "Semantische Rolle" mit einer Entitätsfunktion. Die Funktion trifft also zeitweilig - für das Narrativ - auf den Rolleninhaber (Akteur oder Ding) zu.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Person">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Denotes a person who is involved in a narrative.</dc:description>
    <dc:description xml:lang="de">Bezeichnet eine Person, welche in einem Narrativ involviert ist.</dc:description>
    <rdfs:label xml:lang="de">Person</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/Actor"/>
    <rdfs:label xml:lang="en">Person</rdfs:label>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/PatientRole">
    <rdfs:label xml:lang="de">Rolle Patiens</rdfs:label>
    <dc:description xml:lang="de">Patiens - Die von einem Geschehen betroffene lebende Entität, die keine Kontrolle auf den Vorgang ausübt; die betroffene Sache oder der betroffene Sachverhalt.</dc:description>
    <rdfs:label xml:lang="en">Patient Role</rdfs:label>
    <dc:description xml:lang="en">Patient - The living entity affected by an event or action and not exercising control over the event; the object or matter affected.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/UndergoerRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasTextPassage">
    <dc:description xml:lang="en">This property relates a narrative realised in a text to the specific section of text that describes the narrative in the MHDBDB.</dc:description>
    <rdfs:label xml:lang="de">hat Textpassage</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/TextPassage"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has text passage</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/TextualRealisation"/>
    <dc:description xml:lang="de">Diese Eigenschaft verbindet ein textell realisiertes Narrativ mit dem spezifischen Textausschnitt, der das Narrativ schildert, in der MHDBDB.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ModeRole">
    <dc:description xml:lang="en">"Adverbial of manner – procedure (how?); degree, measure, intensity (how much?); material quality (from what?)</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Adverbiale der Art und Weise – Vorgehensweise (wie?); Grad, Maß, Intensität (wieviel, wie sehr...?); stoffliche Beschaffenheit (woraus?)</dc:description>
    <rdfs:label xml:lang="de">Rolle Modus</rdfs:label>
    <rdfs:label xml:lang="en">Mode Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ParticipatorRole">
    <rdfs:label xml:lang="de">Rolle Teilnehmende/r</rdfs:label>
    <dc:description xml:lang="de">Die Klasse umfasst typischerweise belebte Entitäten, die am Geschehen teilhaben, aber keine Kontrolle oder Initiative im Zusammenhang damit aufweisen.</dc:description>
    <rdfs:label xml:lang="en">Participator Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ActantRole"/>
    <dc:description xml:lang="en">The class includes typicallly animate entities which participate in the event but have no control or initiative associated it.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ActantRole">
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <dc:description xml:lang="de">Aktanten sind Elemente, die eine Handlung ausführen oder von einer Handlung betroffen sind.</dc:description>
    <rdfs:label xml:lang="de">Rolle Aktant</rdfs:label>
    <rdfs:label xml:lang="en">Actant Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Actants are elements that perform or are subjected to an action.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasCidocEquivalent">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label xml:lang="de">hat Cidoc Äquivalent</rdfs:label>
    <rdfs:label xml:lang="en">has Cidoc Equivalent</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/nextSyuzhet">
    <rdfs:label xml:lang="en">next syuzhet</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/next"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the next narrative on the syuzhet path.</dc:description>
    <rdfs:label xml:lang="de">nächstes Syuzhet</rdfs:label>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/previousSyuzhet"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das nächste Narrativ auf dem Syuzhet-Pfad.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N4f13a0ec44cd46a5a284ebda59448084">
    <rdf:rest rdf:nodeID="Nf27a9fb25ee34f24a16520bfcad6f6c3"/>
    <rdf:first rdf:resource="http://onama.sbg.ac.at/Narrative"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasIconClassEquivalent">
    <dc:description xml:lang="de">Dieses Narrativ hat eine IconClass Entsprechung.</dc:description>
    <rdfs:label xml:lang="de">hat IconClass Äquivalent</rdfs:label>
    <rdfs:domain rdf:nodeID="Nb454fc66a47a4cf49da8694aa408ab00"/>
    <dc:description xml:lang="en">This narrative has an IconClass equivalent.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#sameAs"/>
    <rdfs:label xml:lang="en">has IconClass equivalent</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Narrative">
    <onama:hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E5"/>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Narrativ</rdfs:label>
    <dc:description xml:lang="en">Denotes an action, a change of state.</dc:description>
    <rdfs:label xml:lang="en">Narrative</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet eine Handlung, eine Zustandsänderung.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasUrl">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label xml:lang="de">hat Url</rdfs:label>
    <dc:description xml:lang="en">Reference to an URL.</dc:description>
    <rdfs:label xml:lang="en">has Url</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="de">Verweis auf eine URL.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasPlace">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">hat Ort</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="en">This edge connects a semantic role, usually of the "localisation" type, with a place indication.</dc:description>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Place"/>
    <rdfs:label xml:lang="en">has place</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle, meistens vom Typ "Lokalisierung", mit einer Ortsangabe.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasRelatedWikiDataEntry">
    <rdfs:label xml:lang="en">has related WikiData entry</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#closeMatch"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="en">The property hasRelatedWikiDataEntry is used to refer to a similar WikiData entry.</dc:description>
    <rdfs:label xml:lang="de">hat verwandten WikiData Eintrag</rdfs:label>
    <dc:description xml:lang="de">Die Eigenschaft hasRelatedWikiDataEntry wird verwendet, um auf einen ähnlichen WikiData-Eintrag zu verweisen.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TemporalityRole">
    <rdfs:label xml:lang="en">Temporality Role</rdfs:label>
    <dc:description xml:lang="de">Gibt den Zeitraum oder Zeitpunkt eines Geschehens an.</dc:description>
    <rdfs:label xml:lang="de">Rolle Zeitlichkeit</rdfs:label>
    <dc:description xml:lang="en">Indicates the period or time of an event.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/LocalisationRole">
    <dc:description xml:lang="en">Local Averbial; Adverbial of Place.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rolle Lokalisierung</rdfs:label>
    <dc:description xml:lang="de">Lokaladverbiale; Adverbiale des Raumes.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdfs:label xml:lang="en">Localisation Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ThemeRole">
    <dc:description xml:lang="de">Thema – Die Entität, die durch das im Prädikat bezeichnete Geschehen (bzw. den bezeichneten Zustand) räumlich bewegt oder räumlich eingeordnet wird.</dc:description>
    <dc:description xml:lang="en">Theme – The entity that is spatially moved or whose spatial position is indicated by the event (or state) designated in the predicate.</dc:description>
    <rdfs:label xml:lang="en">Theme Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Thema</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/UndergoerRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Thing">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E70_Thing"/>
    <dc:description xml:lang="en">Comprises intellectual products or physical things.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Ding</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/PersistentItem"/>
    <dc:description xml:lang="de">Umfasst intellektuelle Produkte oder physische Dinge.</dc:description>
    <rdfs:label xml:lang="en">Thing</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Place">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/TimeSpan"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Place</rdfs:label>
    <rdfs:label xml:lang="de">Ort</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet Ausdehnungen im Raum, die in der Regel unbeweglich sind (beispielsweise Burg, Garten, Tal...).</dc:description>
    <dc:description xml:lang="en">Describes extensions in space that are usually immobile (for example castle, garden, valley...).</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/UndergoerRole">
    <dc:description xml:lang="de">Die von der im Prädikat benannten Handlung "betroffene" Entität.</dc:description>
    <dc:description xml:lang="en">The "undergoer" of the action or event denoted by the predicate.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Betroffener</rdfs:label>
    <rdfs:label xml:lang="en">Undergoer Role</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ActantRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/ExperiencerRole">
    <dc:description xml:lang="de">Ein/e Wahrnehmende/r ist eine belebte Entität, die eine vom Prädiktat bezeichnete Sinneswahrnehmung oder einen psychischen Zustand erlebt.</dc:description>
    <rdfs:label xml:lang="en">Experiencer Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Wahrnehmende/r</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/ParticipatorRole"/>
    <dc:description xml:lang="en">An Experiencer is an animate entity that experiences a sensory perception or mental state designated by the predicate.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasNarrative">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <dc:description xml:lang="en">This property relates a semantic role to a narrative.</dc:description>
    <rdfs:label xml:lang="en">has narrative</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Narrative"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit einem Narrativ.</dc:description>
    <rdfs:label xml:lang="de">hat Narrativ</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasSigil">
    <dc:description xml:lang="en">This property relates a Collection to a sigil in the MHDBDB.</dc:description>
    <rdfs:label xml:lang="en">has MHDBDB sigil</rdfs:label>
    <rdfs:label xml:lang="de">hat MHDBDB Sigle</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine Sammlung mit einer Sigle in der MHDBDB.</dc:description>
    <rdfs:label xml:lang="en">has sigil</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Collection"/>
    <rdfs:label xml:lang="de">hat Sigle</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/VisualRealisation">
    <rdfs:label xml:lang="en">Visual Realisation</rdfs:label>
    <rdfs:label xml:lang="de">Visuelle Realisierung</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Konzepts in einem Bild.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="en">Refers to the concrete implementation of a concept in an image.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/narrower">
    <dc:description xml:lang="en">This property relates one narrative concept to another narrative concept that can be considered part of the first.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ-Konzept mit einem weiteren Narrativ-Konzept, das als Teil des ersten betrachtet werden kann.</dc:description>
    <rdfs:label xml:lang="en">narrower</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/narrowerTransitive"/>
    <rdfs:label xml:lang="de">enger</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/fromToken">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label xml:lang="en">from token</rdfs:label>
    <rdfs:label xml:lang="de">von Token</rdfs:label>
    <dc:description xml:lang="de">Diese Kante zeigt von einer Textpassage auf das erste Token aus dieser Gruppe.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/TextPassage"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <dc:description xml:lang="en">This property points from a Text Passage to the first token of this group.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/PlaceRole">
    <rdfs:label xml:lang="de">Rolle Ort</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Locative – The specification of the place where the action or event denoted by the predicate is situated.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/LocalisationRole"/>
    <dc:description xml:lang="de">Lokativ – Die Angabe des Ortes, an dem die durch das Prädikat bezeichnete Handlung oder das Ereignis stattfindet.</dc:description>
    <rdfs:label xml:lang="en">Place Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasSemanticRole">
    <rdfs:label xml:lang="de">hat semantische Rolle</rdfs:label>
    <rdfs:label xml:lang="en">has semantic role</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Narrative"/>
    <dc:description xml:lang="en">Basic relation between a narrative and its particular components.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="de">Die grundlegende Beziehung zwischen einem Narrativ und dessen einzelnen "Bausteinen".</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/broaderTransitive">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:label xml:lang="de">transitiv umfassender</rdfs:label>
    <rdfs:label xml:lang="en">broader transitive</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet ein Konzept eines Narrativs mit sämtlichen übergeordneten Konzepten.</dc:description>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/isRelatedTo"/>
    <dc:description xml:lang="en">This property relates a narrative concept to all the hierarchically superordinated narrative concepts.</dc:description>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/narrowerTransitive"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nf27a9fb25ee34f24a16520bfcad6f6c3">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="http://onama.sbg.ac.at/PersistentItem"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Verb">
    <dc:description xml:lang="de">Die Klasse "Verb" umfasst Handlungen, die die Zuweisung semantischer Rollen ermöglichen.</dc:description>
    <rdfs:label xml:lang="en">Verb</rdfs:label>
    <rdfs:label xml:lang="de">Verb</rdfs:label>
    <dc:description xml:lang="en">The class "verb" includes actions that allow the assignment of semantic roles.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TimeSpan">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E52_Time-Span"/>
    <dc:description xml:lang="de">Diese Klasse umfasst abstrakte zeitliche Umfänge, die einen Anfang, ein Ende und eine Dauer haben.</dc:description>
    <rdfs:label xml:lang="de">Zeitspanne</rdfs:label>
    <dc:description xml:lang="en">This class comprises abstract temporal extents having a beginning, an end and a duration.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Time Span</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/PredicativeRole">
    <dc:description xml:lang="en">E.g. "The princes elect Frederick as king." – "as king": predicative.</dc:description>
    <rdfs:label xml:lang="en">Predicative Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <dc:description xml:lang="de">Z.B. "Die Fürsten wählen Friedrich zum König." – "zum König": Prädikativ.</dc:description>
    <rdfs:label xml:lang="de">Rolle Prädikativ</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/TemporalEntity">
    <dc:description xml:lang="en">This class comprises all phenomena, such as periods, events and states, which happen over a limited extent in time; it is disjoint from Persistent Item.</dc:description>
    <rdfs:label xml:lang="de">Zeitliche Entität (Geschehendes)</rdfs:label>
    <rdfs:label xml:lang="en">Temporal Entity</rdfs:label>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/sites/default/files/Documents/cidoc_crm_version_5.0.4.html#E2"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Diese  Klasse  umfasst  Phasen, Ereignisse  und  andere  zeitlich  begrenzte Zustände; sie ist disjunkt von der Klasse "Persistentes Objekt (Seiendes)".</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/previous">
    <rdfs:label xml:lang="de">vorhergehend</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/isRelatedTo"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">previous</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ.</dc:description>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the previous narrative.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/PersistentItem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Persistentes Objekt (Seiendes)</rdfs:label>
    <dc:description xml:lang="de">Diese Klasse umfasst Lebewesen, Gegenstände und geistige Erzeugnisse, die eine dauerhafte Identität haben.</dc:description>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/Place"/>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/TimeSpan"/>
    <dc:description xml:lang="en">This class includes living beings, objects and intellectual products which have a permanent identity.</dc:description>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E77_Persistent_Item"/>
    <owl:disjointWith rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <rdfs:label xml:lang="en">Persistent Item</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasVerb">
    <dc:description xml:lang="en">This property relates an action with the corresponding verb.</dc:description>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Action"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine Handlung mit dem entsprechenden Verb.</dc:description>
    <rdfs:label xml:lang="de">hat Verb</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Verb"/>
    <rdfs:label xml:lang="en">has verb</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </rdf:Description>
