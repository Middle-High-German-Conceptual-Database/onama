    <rdfs:label xml:lang="de">hat Handlung</rdfs:label>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Action"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Realisation">
    <rdfs:label xml:lang="de">Realisierung</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Narrativ-Konzepts in einem spezifischen Medium, etwa einem Text oder einem Bild.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/Narrative"/>
    <rdfs:label xml:lang="en">Realisation</rdfs:label>
    <dc:description xml:lang="en">Denotes the concrete implementation of a narrative concept in a specific medium, such as a text or an image.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/StimulusRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Stimulus</rdfs:label>
    <dc:description xml:lang="en">The often inanimate trigger of the situation described by the verbal predicate; in combination with the Experiencer Role: the object of a mental state.</dc:description>
    <dc:description xml:lang="de">Der oftmals unbelebte Auslöser der vom Prädikat bezeichneten Situation; in Kombination mit der Rolle Experiencer (Wahrnehmende/r): der Gegenstand eines psychischen Zustands.</dc:description>
    <rdfs:subClassOf rdf:resource="http://onama.sbg.ac.at/CauserRole"/>
    <rdfs:label xml:lang="en">Stimulus Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/isRelatedTo">
    <dc:description xml:lang="en">The basic property to state a relation between two narratives.</dc:description>
    <rdfs:label xml:lang="en">is related to</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Die grundlegende Kante, um eine Beziehung zwischen zwei Narrativen herzustellen.</dc:description>
    <rdfs:label xml:lang="de">steht in Beziehung mit</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/hasTemporalEntity">
    <dc:description xml:lang="en">This property relates a semantic role to a temporal entity that holds this role.</dc:description>
    <rdfs:label xml:lang="en">has temporal entity</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">hat zeitliche Entität (Geschehendes)</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/SemanticRole"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit einer zeitlichen Entität, die diese Rolle besetzt.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/TemporalEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/isRealisedAsSupplementaryNarrativeBy">
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:label xml:lang="de">ist realisiert als ergänzendes Narrativ durch</rdfs:label>
    <dc:description xml:lang="de">Diese Eigenschaft verbindet jene Konzepte, die eine "Haupthandlung" ergänzen, mit ihren Realisierungen.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">This property relates those concepts that are supplementary to a "main plot" to their realisations.</dc:description>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/realisesAsSupplementaryNarrative"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="en">is realised as supplementary narrative by</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://onama.sbg.ac.at/isRealisedBy"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/isRealisedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="de">Das Narrativ ist realisiert durch ein spezifisches Werk der Literatur oder der bildenden Kunst; diese Kante verbindet das Konzept eines Narrativs mit dessen Realisierung.</dc:description>
    <rdfs:label xml:lang="en">is realised by</rdfs:label>
    <rdfs:domain rdf:resource="http://onama.sbg.ac.at/Concept"/>
    <rdfs:range rdf:resource="http://onama.sbg.ac.at/Realisation"/>
    <rdfs:label xml:lang="de">ist realisiert durch</rdfs:label>
    <dc:description xml:lang="en">The narrative is realized through a specific work of literature or the visual arts; this property relates the concept of a narrative to its realisation.</dc:description>
    <owl:inverseOf rdf:resource="http://onama.sbg.ac.at/realises"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://onama.sbg.ac.at/Group">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Denotes a group of individuals who are involved in a narrative.</dc:description>
    <rdfs:label xml:lang="de">Menschliche Gruppe</rdfs:label>
