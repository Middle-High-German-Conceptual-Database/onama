  <rdf:Description rdf:about="TemporalEntity">
    <dc:description xml:lang="de">Diese  Klasse  umfasst  Phasen, Ereignisse  und  andere  zeitlich  begrenzte Zustände; sie ist disjunkt von der Klasse "Persistentes Objekt (Seiendes)".</dc:description>
    <rdfs:label xml:lang="de">Zeitliche Entität (Geschehendes)</rdfs:label>
    <dc:description xml:lang="en">This class comprises all phenomena, such as periods, events and states, which happen over a limited extent in time; it is disjoint from Persistent Item.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/sites/default/files/Documents/cidoc_crm_version_5.0.4.html#E2"/>
    <rdfs:label xml:lang="en">Temporal Entity</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#sameAs">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:label xml:lang="de">identisch mit</rdfs:label>
    <rdfs:label xml:lang="en">same as</rdfs:label>
    <dc:description xml:lang="de">Verweist auf Äquivalente in Normdaten und Klassifizierungkonzepten.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="en">Refers to equivalents in authority files and classification concepts.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="broader">
    <dc:description xml:lang="de">Die Kante "umfassender" verweist von einem Konzept eines Narraivs auf das unmittelbar übergeordnete Konzept und zeigt damit an, dass ein Narrativ Teil eines anderen Narrativs ist.</dc:description>
    <rdfs:range rdf:resource="Concept"/>
    <rdfs:subPropertyOf rdf:resource="broaderTransitive"/>
    <rdfs:label xml:lang="en">broader</rdfs:label>
    <owl:inverseOf rdf:resource="narrower"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="Concept"/>
    <rdfs:label xml:lang="de">umfassender</rdfs:label>
    <dc:description xml:lang="en">The property "broader" relates a narrative concept to the immediately superordinate concept, indicating that a narrative is part of another narrative.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="depictedIn">
    <rdfs:domain rdf:resource="VisualRealisation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine visuelle Realisierung mit dem Bild-Objekt, auf dem diese dargestellt ist, z.B. in REALonline.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <dc:description xml:lang="en">This property relates a visual realisation to the image object on which it is displayed, e.g. in REALonline.</dc:description>
    <rdfs:label xml:lang="en">depicted in</rdfs:label>
    <rdfs:label xml:lang="de">abgebildet in</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="WitnessRole">
    <rdfs:subClassOf rdf:resource="ParticipatorRole"/>
    <dc:description xml:lang="de">Zeuge – Die belebte Entität, die Zeuge der Handlung oder des Ereignisses ist, das durch das Prädikat bezeichnet wird.</dc:description>
    <rdfs:label xml:lang="en">Witness Role</rdfs:label>
    <dc:description xml:lang="en">Witness – The animate entity that witnesses the action or event denoted by the predicate.</dc:description>
    <rdfs:label xml:lang="de">Rolle Zeuge</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="TextualRealisation">
    <rdfs:label xml:lang="en">Textual Realisation</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Konzepts in einem Text.</dc:description>
    <owl:disjointWith rdf:resource="VisualRealisation"/>
    <dc:description xml:lang="en">Denotes the concrete implementation of a concept in a text.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Textuelle Realisierung</rdfs:label>
    <rdfs:subClassOf rdf:resource="Realisation"/>
  </rdf:Description>
  <rdf:Description rdf:about="Co-AgentRole">
    <dc:description xml:lang="en">Co- or Re-Agent in verbs that describe actions that are in the relation of action and reaction to each other.</dc:description>
    <rdfs:label xml:lang="en">Co-Agent Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Ko-Agens</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="CauserRole"/>
    <dc:description xml:lang="de">Ko- oder Re-Agierender bei Verben, die Handlungen beschreiben, die in der Relation von Aktion und Reaktion zueinander stehen.</dc:description>
  </rdf:Description>
