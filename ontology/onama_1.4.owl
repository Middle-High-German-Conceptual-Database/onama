  <rdf:Description rdf:about="occursSimultaneously">
    <rdfs:range rdf:resource="Realisation"/>
    <dc:description xml:lang="en">On the level of realisation, this property relates one narrative to another narrative that takes place simultaneously.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet auf Realisierungsebene ein Narrativ mit einem anderen Narrativ, das gleichzeitig stattfindet.</dc:description>
    <rdfs:label xml:lang="en">occurs simultaneously</rdfs:label>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <rdfs:label xml:lang="de">passiert gleichzeitig</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N83eba2bc0ddc404ea76621dc1de1e222">
    <rdf:first rdf:resource="PersistentItem"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="realisesAsSupplementaryNarrative">
    <dc:description xml:lang="en">This property relates narrative realisations that are supplementary to a "main plot" to their respective concepts.</dc:description>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdfs:subPropertyOf rdf:resource="realises"/>
    <rdfs:label xml:lang="de">realisiert als ergänzendes Narrativ</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet Narrativ-Realisierungen, die eine "Haupthandlung" ergänzen, mit ihren jeweiligen Konzepten.</dc:description>
    <rdfs:label xml:lang="en">realises as supplementary narrative</rdfs:label>
    <rdfs:range rdf:resource="Concept"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N4e049de9daff4aa2a632bab6724f7213">
    <rdf:first rdf:resource="PersistentItem"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasTemporalEntity">
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit einer zeitlichen Entität, die diese Rolle besetzt.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:domain rdf:resource="SemanticRole"/>
    <dc:description xml:lang="en">This property relates a semantic role to a temporal entity that holds this role.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has temporal entity</rdfs:label>
    <rdfs:range rdf:resource="TemporalEntity"/>
    <rdfs:label xml:lang="de">hat zeitliche Entität (Geschehendes)</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="broaderTransitive">
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <rdfs:domain rdf:resource="Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <owl:inverseOf rdf:resource="narrowerTransitive"/>
    <dc:description xml:lang="de">Diese Kante verbindet ein Konzept eines Narrativs mit sämtlichen übergeordneten Konzepten.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">This property relates a narrative concept to all the hierarchically superordinated narrative concepts.</dc:description>
    <rdfs:label xml:lang="en">broader transitive</rdfs:label>
    <rdfs:label xml:lang="de">transitiv umfassender</rdfs:label>
    <rdfs:range rdf:resource="Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="Person">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Bezeichnet eine Person, welche in einem Narrativ involviert ist.</dc:description>
    <dc:description xml:lang="en">Denotes a person who is involved in a narrative.</dc:description>
    <rdfs:label xml:lang="de">Person</rdfs:label>
    <rdfs:label xml:lang="en">Person</rdfs:label>
    <rdfs:subClassOf rdf:resource="Actor"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasMhdbdbSigilURL">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="hasUrl"/>
    <dc:description xml:lang="de">Verweis auf eine die URL zu einer MHDBDB Sigle.</dc:description>
    <rdfs:label xml:lang="de">hat URL auf MHDBDB Sigle</rdfs:label>
    <dc:description xml:lang="en">Reference to the URL of a MHDBDB sigil.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:domain rdf:resource="Collection"/>
    <rdfs:label xml:lang="en">has Mhdbdb Sigil URL</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="hasNarrative">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Narrative"/>
    <rdfs:label xml:lang="de">hat Narrativ</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit einem Narrativ.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="en">has narrative</rdfs:label>
    <dc:description xml:lang="en">This property relates a semantic role to a narrative.</dc:description>
    <rdfs:domain rdf:resource="SemanticRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="Concept">
    <dc:description xml:lang="de">Definiert ein spezifisches Narrativ als Abstraktum ohne einen konkreten Rückbezug auf eine Realisierung.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:disjointWith rdf:resource="Realisation"/>
    <rdfs:subClassOf rdf:resource="Narrative"/>
    <rdfs:label xml:lang="de">Konzept</rdfs:label>
    <rdfs:label xml:lang="en">Concept</rdfs:label>
    <dc:description xml:lang="en">Defines a specific narrative as an abstraction without a concrete reference to a realization.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="SourceRole">
    <dc:description xml:lang="de">Quelle - Der Ort oder die Entität, von der aus sich etwas bewegt.</dc:description>
    <rdfs:label xml:lang="de">Rolle Quelle</rdfs:label>
    <rdfs:subClassOf rdf:resource="LocalisationRole"/>
    <dc:description xml:lang="en">Source – The location or entity from which something moves.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Source Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="realises">
    <rdfs:label xml:lang="de">realisiert</rdfs:label>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Concept"/>
    <rdfs:label xml:lang="en">realises</rdfs:label>
    <dc:description xml:lang="en">This property relates the realisation of a narrative with the concept of that narrative.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet die Realisierung eines Narrativs mit dem Konzept dieses Narrativs.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="ActantRole">
    <rdfs:label xml:lang="de">Rolle Aktant</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="SemanticRole"/>
    <dc:description xml:lang="de">Aktanten sind Elemente, die eine Handlung ausführen oder von einer Handlung betroffen sind.</dc:description>
    <dc:description xml:lang="en">Actants are elements that perform or are subjected to an action.</dc:description>
    <rdfs:label xml:lang="en">Actant Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="hasIconClassEquivalent">
    <rdfs:label xml:lang="de">hat IconClass Äquivalent</rdfs:label>
    <rdfs:domain rdf:nodeID="N64311045d66f45a9914ef52e4a8e1b73"/>
    <dc:description xml:lang="de">Dieses Narrativ hat eine IconClass Entsprechung.</dc:description>
    <dc:description xml:lang="en">This narrative has an IconClass equivalent.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label xml:lang="en">has IconClass equivalent</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#sameAs"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasGNDEquivalent">
    <dc:description xml:lang="de">Diese Eigenschaft/dieses Narrativ hat eine GND Entsprechung.</dc:description>
    <rdfs:label xml:lang="en">has GND equivalent</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="en">This property/narrative has a GND equivalent.</dc:description>
    <rdfs:label xml:lang="de">hat GND Äquivalent</rdfs:label>
    <rdfs:domain rdf:nodeID="N163d4f71f0ea4a3781ea9403a6fd12c1"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#sameAs"/>
  </rdf:Description>
  <rdf:Description rdf:about="nextSyuzhet">
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das nächste Narrativ auf dem Syuzhet-Pfad.</dc:description>
    <rdfs:label xml:lang="en">next syuzhet</rdfs:label>
    <rdfs:range rdf:resource="Realisation"/>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the next narrative on the syuzhet path.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="previousSyuzhet"/>
    <rdfs:label xml:lang="de">nächstes Syuzhet</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="next"/>
    <rdfs:domain rdf:resource="Realisation"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N82f2b0114bdc47a89f80ee74e4a3ac6d">
    <rdf:first rdf:resource="Place"/>
    <rdf:rest rdf:nodeID="N839982a44be04cd6acafdf50b429088e"/>
  </rdf:Description>
  <rdf:Description rdf:about="next">
    <rdfs:label xml:lang="de">nächstes</rdfs:label>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the next narrative.</dc:description>
    <owl:inverseOf rdf:resource="previous"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdfs:label xml:lang="en">next</rdfs:label>
    <rdfs:range rdf:resource="Realisation"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das nächste Narrativ.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="hasSupplementaryConcept">
    <dc:description xml:lang="en">This property relates a narrative concept to a supplementary narrative concept. A supplementary concept is a narrative, which is presented in addition to the respective "main action".</dc:description>
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ-Konzept mit einem ergänzenden Narrativ-Konzept. Ein ergänzendes Konzept ist ein Narrativ, das zusätzlich zur jeweiligen "Haupthandlung" geschildert wird.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Concept"/>
    <rdfs:label xml:lang="en">has supplementary concept</rdfs:label>
    <rdfs:domain rdf:resource="Concept"/>
    <rdfs:label xml:lang="de">hat ergänzendes Konzept</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="previous">
    <rdfs:label xml:lang="de">vorhergehend</rdfs:label>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the previous narrative.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <rdfs:domain rdf:resource="Realisation"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ.</dc:description>
    <rdfs:label xml:lang="en">previous</rdfs:label>
    <rdfs:range rdf:resource="Realisation"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasAction">
    <rdfs:domain rdf:resource="Narrative"/>
    <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ mit der Handlung, die im Narrativ stattfindet.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Action"/>
    <rdfs:label xml:lang="de">hat Handlung</rdfs:label>
    <rdfs:label xml:lang="en">has action</rdfs:label>
    <dc:description xml:lang="en">This property relates a narrative to the action taking place in the narrative.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="UndergoerRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">The "undergoer" of the action or event denoted by the predicate.</dc:description>
    <dc:description xml:lang="de">Die von der im Prädikat benannten Handlung "betroffene" Entität.</dc:description>
    <rdfs:label xml:lang="en">Undergoer Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Betroffener</rdfs:label>
    <rdfs:subClassOf rdf:resource="ActantRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasTemporaryEntityFunction">
    <dc:description xml:lang="de">Diese Kante verbindet einen Knoten vom Typ "Semantische Rolle" mit einer Entitätsfunktion. Die Funktion trifft also zeitweilig - für das Narrativ - auf den Rolleninhaber (Akteur oder Ding) zu.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">hat zeitweilige Entitätsfunktion</rdfs:label>
    <rdfs:domain rdf:resource="SemanticRole"/>
    <rdfs:label xml:lang="en">has temporary entity function</rdfs:label>
    <dc:description xml:lang="en">This property relates a node of type "Semantic Role" to an entity function. The function thus applies temporarily - for the narrative - to the actor or thing that holds the role.</dc:description>
    <rdfs:subPropertyOf rdf:resource="hasEntityFunction"/>
    <rdfs:range rdf:resource="EntityFunction"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasTextPassage">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="de">hat Textpassage</rdfs:label>
    <rdfs:range rdf:resource="TextPassage"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has text passage</rdfs:label>
    <dc:description xml:lang="en">This property relates a narrative realised in a text to the specific section of text that describes the narrative in the MHDBDB.</dc:description>
    <rdfs:domain rdf:resource="TextualRealisation"/>
    <dc:description xml:lang="de">Diese Eigenschaft verbindet ein textell realisiertes Narrativ mit dem spezifischen Textausschnitt, der das Narrativ schildert, in der MHDBDB.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="previousSyuzhet">
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the prevoious narrative on the syuzhet path.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">previous syuzhet</rdfs:label>
    <rdfs:range rdf:resource="Realisation"/>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdfs:subPropertyOf rdf:resource="previous"/>
    <rdfs:label xml:lang="de">vorhergehendes Syuzhet</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ auf dem Syuzhet-Pfad.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">close match</rdfs:label>
    <dc:description xml:lang="en">The property skos:closeMatch is used to link two concepts that are sufficiently similar that they can be used interchangeably in some information retrieval applications.
(https://www.w3.org/TR/2009/REC-skos-reference-20090818/#mapping)</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <dc:description xml:lang="de">Die Eigenschaft skos:closeMatch wird verwendet, um zwei Konzepte zu verknüpfen, die sich so ähnlich sind, dass sie in einigen Anwendungen zur Informationsbeschaffung austauschbar verwendet werden können.
(https://www.w3.org/TR/2009/REC-skos-reference-20090818/#mapping)</dc:description>
    <rdfs:label xml:lang="de">Entsprechung mit großer Ähnlichkeit</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="narrowerTransitive">
    <rdfs:subPropertyOf rdf:resource="isRelatedTo"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">narrower transitive</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <dc:description xml:lang="de">Diese Kante verweist von einem Narrativ-Konzept auf sämtliche untergeordneten Narrativ-Konzepte.</dc:description>
    <rdfs:range rdf:resource="Concept"/>
    <rdfs:domain rdf:resource="Concept"/>
    <dc:description xml:lang="en">This property points from one narrative concept to all subordinate narrative concepts.</dc:description>
    <rdfs:label xml:lang="de">transitiv enger</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="TextPassage">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Textpassage</rdfs:label>
    <onama:hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E33"/>
    <dc:description xml:lang="de">Bezeichnet einen Ausschnitt aus einem Text; beispielsweise in der Mittelhochdeutschen Begriffsdatenbank (MHDBDB).</dc:description>
    <rdfs:label xml:lang="en">Text Passage</rdfs:label>
    <dc:description xml:lang="en">Denotes a section from a text; for instance in the Mittelhochdeutsche Begriffsdatenbank (MHDBDB).</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="hasUrl">
    <rdfs:label xml:lang="en">has Url</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <dc:description xml:lang="de">Verweis auf eine URL.</dc:description>
    <dc:description xml:lang="en">Reference to an URL.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="de">hat Url</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="hasSemanticRole">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="en">has semantic role</rdfs:label>
    <dc:description xml:lang="en">Basic relation between a narrative and its particular components.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="SemanticRole"/>
    <rdfs:label xml:lang="de">hat semantische Rolle</rdfs:label>
    <rdfs:domain rdf:resource="Narrative"/>
    <dc:description xml:lang="de">Die grundlegende Beziehung zwischen einem Narrativ und dessen einzelnen "Bausteinen".</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="PersistentItem">
    <rdfs:label xml:lang="de">Persistentes Objekt (Seiendes)</rdfs:label>
    <owl:disjointWith rdf:resource="SemanticRole"/>
    <owl:disjointWith rdf:resource="Place"/>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E77_Persistent_Item"/>
    <owl:disjointWith rdf:resource="TimeSpan"/>
    <rdfs:label xml:lang="en">Persistent Item</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">This class includes living beings, objects and intellectual products which have a permanent identity.</dc:description>
    <dc:description xml:lang="de">Diese Klasse umfasst Lebewesen, Gegenstände und geistige Erzeugnisse, die eine dauerhafte Identität haben.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="Collection">
    <rdfs:label xml:lang="de">Sammlung</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Identifies the textual or pictorial context from which the narrative originates.</dc:description>
    <dc:description xml:lang="de">Benennt den textlichen oder bildlichen Kontext, aus welchem das Narrativ stammt.</dc:description>
    <rdfs:label xml:lang="en">Collection</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="EntityFunction">
    <dc:description xml:lang="en">Denotes a function of an actor or thing in a narrative (for example hero, ruler, villain, trophy, talisman...).</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Bezeichnet eine Funktion eines Akteurs oder  Dings in einem Narrativ (beispielsweise Held, Herrscher, Bösewicht, Trophäe, Talisman...).</dc:description>
    <rdfs:label xml:lang="en">Entity Function</rdfs:label>
    <rdfs:label xml:lang="de">Entitätsfunktion</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="Narrative">
    <dc:description xml:lang="de">Bezeichnet eine Handlung, eine Zustandsänderung.</dc:description>
    <rdfs:label xml:lang="de">Narrativ</rdfs:label>
    <owl:disjointWith rdf:resource="SemanticRole"/>
    <onama:hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E5"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Denotes an action, a change of state.</dc:description>
    <rdfs:label xml:lang="en">Narrative</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="CauserRole">
    <dc:description xml:lang="en">Causer is the semantic role of a person or thing who is the doer or the instigator of an event.</dc:description>
    <rdfs:label xml:lang="de">Rolle Verursacher</rdfs:label>
    <dc:description xml:lang="de">Verursacher ist die semantische Rolle einer Person oder Sache, die eine Handlung ausführt bzw. ein Ereignis auslöst.</dc:description>
    <rdfs:subClassOf rdf:resource="ActantRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Causer Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="PossessorRole">
    <rdfs:subClassOf rdf:resource="ParticipatorRole"/>
    <rdfs:label xml:lang="de">Rolle Besitzer</rdfs:label>
    <rdfs:label xml:lang="en">Possessor Role</rdfs:label>
    <dc:description xml:lang="en">Possessor is the semantic role of an entity that possesses something.</dc:description>
    <dc:description xml:lang="de">Besitzer ist die semantische Rolle einer Entität, die etwas besitzt.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="firstInCollection">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="en">This property relates a collection with the first narrative (realisation) in the collection.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:range rdf:resource="Realisation"/>
    <rdfs:label xml:lang="de">erstes Element in einer Sammlung</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet eine Sammlung mit dem ersten Narrativ (Realisierung) in der Sammlung.</dc:description>
    <rdfs:label xml:lang="en">first in collection</rdfs:label>
    <rdfs:domain rdf:resource="Collection"/>
  </rdf:Description>
  <rdf:Description rdf:about="Place">
    <rdfs:label xml:lang="de">Ort</rdfs:label>
    <dc:description xml:lang="en">Describes extensions in space that are usually immobile (for example castle, garden, valley...).</dc:description>
    <dc:description xml:lang="de">Bezeichnet Ausdehnungen im Raum, die in der Regel unbeweglich sind (beispielsweise Burg, Garten, Tal...).</dc:description>
    <owl:disjointWith rdf:resource="TimeSpan"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <rdfs:label xml:lang="en">Place</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N163d4f71f0ea4a3781ea9403a6fd12c1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="N7af2fe6579a1469382a5323aba276785"/>
  </rdf:Description>
  <rdf:Description rdf:about="Thing">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E70_Thing"/>
    <dc:description xml:lang="en">Comprises intellectual products or physical things.</dc:description>
    <rdfs:label xml:lang="en">Thing</rdfs:label>
    <rdfs:subClassOf rdf:resource="PersistentItem"/>
    <rdfs:label xml:lang="de">Ding</rdfs:label>
    <dc:description xml:lang="de">Umfasst intellektuelle Produkte oder physische Dinge.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="narrower">
    <rdfs:domain rdf:resource="Concept"/>
    <rdfs:subPropertyOf rdf:resource="narrowerTransitive"/>
    <rdfs:label xml:lang="de">enger</rdfs:label>
    <dc:description xml:lang="de">Diese Kante verbindet ein Narrativ-Konzept mit einem weiteren Narrativ-Konzept, das als Teil des ersten betrachtet werden kann.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Concept"/>
    <rdfs:label xml:lang="en">narrower</rdfs:label>
    <dc:description xml:lang="en">This property relates one narrative concept to another narrative concept that can be considered part of the first.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="Group">
    <rdfs:label xml:lang="en">Group</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Menschliche Gruppe</rdfs:label>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
    <rdfs:subClassOf rdf:resource="Actor"/>
    <dc:description xml:lang="en">Denotes a group of individuals who are involved in a narrative.</dc:description>
    <dc:description xml:lang="de">Bezeichnet eine Gruppe von Individuen, die in einem Narrativ involviert sind.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nb277bb4570e54d69aecfc06bca745473">
    <rdf:first rdf:resource="TemporalEntity"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="PlaceRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Ort</rdfs:label>
    <rdfs:subClassOf rdf:resource="LocalisationRole"/>
    <dc:description xml:lang="de">Lokativ – Die Angabe des Ortes, an dem die durch das Prädikat bezeichnete Handlung oder das Ereignis stattfindet.</dc:description>
    <dc:description xml:lang="en">Locative – The specification of the place where the action or event denoted by the predicate is situated.</dc:description>
    <rdfs:label xml:lang="en">Place Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="TimeSpan">
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E52_Time-Span"/>
    <dc:description xml:lang="en">This class comprises abstract temporal extents having a beginning, an end and a duration.</dc:description>
    <rdfs:label xml:lang="de">Zeitspanne</rdfs:label>
    <dc:description xml:lang="de">Diese Klasse umfasst abstrakte zeitliche Umfänge, die einen Anfang, ein Ende und eine Dauer haben.</dc:description>
    <rdfs:label xml:lang="en">Time Span</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="RecipientRole">
    <dc:description xml:lang="de">Der Empfänger einer Entität oder Information bei Verben, die den Transfer eines Besitzgegenstandes oder einer Information bezeichnen.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Rezipient</rdfs:label>
    <rdfs:subClassOf rdf:resource="ParticipatorRole"/>
    <dc:description xml:lang="en">The recipient of an entity or information in verbs that denote the transfer of an object of possession or information.</dc:description>
    <rdfs:label xml:lang="en">Recipient Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="partOfCollection">
    <dc:description xml:lang="de">Diese Kante verweist von einer Narrativ-Realisierung auf die Sammlung, aus der das Narrativ stammt.</dc:description>
    <rdfs:range rdf:resource="Collection"/>
    <dc:description xml:lang="en">This property points from a narrative realisation to the collection from which the narrative originates.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="de">Teil einer Sammlung</rdfs:label>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdfs:label xml:lang="en">part of collection</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="GoalRole">
    <rdfs:label xml:lang="en">Goal Role</rdfs:label>
    <dc:description xml:lang="de">Ziel - Der Ort oder die Entität, in deren Richtung sich etwas bewegt.</dc:description>
    <rdfs:label xml:lang="de">Rolle Ziel</rdfs:label>
    <dc:description xml:lang="en">Goal – The location or entity in the direction of which something moves.</dc:description>
    <rdfs:subClassOf rdf:resource="LocalisationRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="fromToken">
    <dc:description xml:lang="de">Diese Kante zeigt von einer Textpassage auf das erste Token aus dieser Gruppe.</dc:description>
    <rdfs:label xml:lang="de">von Token</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <dc:description xml:lang="en">This property points from a Text Passage to the first token of this group.</dc:description>
    <rdfs:label xml:lang="en">from token</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="TextPassage"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="BenefactiveRole">
    <rdfs:label xml:lang="en">Benefactive Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Benefaktiv ist die semantische Rolle eines Referenten, der durch ein Ereignis begünstigt oder benachteiligt wird.</dc:description>
    <rdfs:label xml:lang="de">Rolle Benefaktiv</rdfs:label>
    <dc:description xml:lang="en">Benefactive is the semantic role of a referent which is advantaged or disadvantaged by an event.</dc:description>
    <rdfs:subClassOf rdf:resource="ParticipatorRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasEntityFunction">
    <rdfs:label xml:lang="de">hat Entitätsfunktion</rdfs:label>
    <dc:description xml:lang="en">This property relates an actor or a thing to an Entity Function. Please use one of the two subproperties.</dc:description>
    <rdfs:range rdf:resource="EntityFunction"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet einen Akteur, ein Ding oder eine Semantische Rolle mit einer Entitätsfunktion. Bitte verwenden Sie eine der beiden Subproperties.</dc:description>
    <rdfs:label xml:lang="en">has Entity Function</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="Realisation">
    <rdfs:subClassOf rdf:resource="Narrative"/>
    <rdfs:label xml:lang="en">Realisation</rdfs:label>
    <rdfs:label xml:lang="de">Realisierung</rdfs:label>
    <dc:description xml:lang="en">Denotes the concrete implementation of a narrative concept in a specific medium, such as a text or an image.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Bezeichnet die konkrete Umsetzung eines Narrativ-Konzepts in einem spezifischen Medium, etwa einem Text oder einem Bild.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="hasActor">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet eine semantische Rolle mit dem Akteur, der diese Rolle besetzt.</dc:description>
    <rdfs:domain rdf:resource="SemanticRole"/>
    <dc:description xml:lang="en">This property relates a semantic role to the actor who holds this role.</dc:description>
    <rdfs:label xml:lang="de">hat Akteur</rdfs:label>
    <rdfs:range rdf:resource="Actor"/>
    <rdfs:label xml:lang="en">has actor</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasGeneralEntityFunction">
    <dc:description xml:lang="en">This property relates an actor or a thing to a generally applicable entity function.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description xml:lang="de">Diese Kante verbindet einen Akteur oder ein Ding mit einer allgemein zutreffenden Entitätsfunktion.</dc:description>
    <rdfs:subPropertyOf rdf:resource="hasEntityFunction"/>
    <rdfs:label xml:lang="de">hat allgemeine Entitätsfunktion</rdfs:label>
    <rdfs:domain rdf:resource="PersistentItem"/>
    <rdfs:range rdf:resource="EntityFunction"/>
    <rdfs:label xml:lang="en">has general entity function</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="isRealisedBy">
    <rdfs:label xml:lang="de">ist realisiert durch</rdfs:label>
    <rdfs:domain rdf:resource="Concept"/>
    <rdfs:label xml:lang="en">is realised by</rdfs:label>
    <dc:description xml:lang="de">Das Narrativ ist realisiert durch ein spezifisches Werk der Literatur oder der bildenden Kunst; diese Kante verbindet das Konzept eines Narrativs mit dessen Realisierung.</dc:description>
    <dc:description xml:lang="en">The narrative is realized through a specific work of literature or the visual arts; this property relates the concept of a narrative to its realisation.</dc:description>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <owl:inverseOf rdf:resource="realises"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="Realisation"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasRelatedIconClassEntry">
    <dc:description xml:lang="de">Die Eigenschaft onama:hasRelatedIconClassEntry wird verwendet, um auf einen ähnlichen IconClass-Eintrag zu verweisen.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#closeMatch"/>
    <dc:description xml:lang="en">The property onama:hasRelatedIconClassEntry is used to refer to a similar IconClass entry.</dc:description>
    <rdfs:label xml:lang="en">has related IconClass entry</rdfs:label>
    <rdfs:label xml:lang="de">hat verwandten IconClass Eintrag</rdfs:label>
    <rdfs:domain rdf:nodeID="N68e8b9a4059e4993a6057b373b2087b0"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="hasThing">
    <rdfs:label xml:lang="de">hat Ding</rdfs:label>
    <rdfs:range rdf:resource="Thing"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has thing</rdfs:label>
    <rdfs:domain rdf:resource="SemanticRole"/>
    <dc:description xml:lang="en">This property relates a semantic role to an intellectual product or physical thing that holds this role.</dc:description>
    <dc:description xml:lang="de">Diese Kante verbindet eine sematische Rolle mit einem intellektuellen Produkt oder physischen Ding, das diese Rolle besetzt.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="Verb">
    <dc:description xml:lang="de">Die Klasse "Verb" umfasst Handlungen, die die Zuweisung semantischer Rollen ermöglichen.</dc:description>
    <rdfs:label xml:lang="de">Verb</rdfs:label>
    <dc:description xml:lang="en">The class "verb" includes actions that allow the assignment of semantic roles.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Verb</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="InstrumentRole">
    <rdfs:subClassOf rdf:resource="SemanticRole"/>
    <dc:description xml:lang="en">Instrument – The medium by which the action or event denoted by the predicate is carried out.</dc:description>
    <dc:description xml:lang="de">Instrument - Das Mittel, mit dem die durch das Prädikat bezeichnete Handlung oder das Ereignis ausgeführt wird.</dc:description>
    <rdfs:label xml:lang="en">Instrument Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="de">Rolle Instrument</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="previousFabula">
    <rdfs:range rdf:resource="Realisation"/>
    <onama:hasCidocEquivalent rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#P120i"/>
    <rdfs:label xml:lang="en">previous fabula</rdfs:label>
    <rdfs:domain rdf:resource="Realisation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="previous"/>
    <dc:description xml:lang="de">Diese Kante verweist auf Realisierungsebene von einem Narrativ auf das vorhergehende Narrativ auf dem Fabula-Pfad.</dc:description>
    <rdfs:label xml:lang="de">vorhergehende Fabula</rdfs:label>
    <dc:description xml:lang="en">On the level of realisation, this property points from one narrative to the previous narrative on the fabula path.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="MythicalBeing">
    <dc:description xml:lang="en">Denotes a mythical being or a deity that is involved in a narrative.</dc:description>
    <rdfs:subClassOf rdf:resource="Actor"/>
    <rdfs:label xml:lang="en">MythicalBeing</rdfs:label>
    <dc:description xml:lang="de">Bezeichnet ein mythisches Wesen, eine Gottheit oder ein Fabelwesen, welches in einem Narrativ involviert ist.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <onama:hasCidocSuperclass rdf:resource="http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#E28"/>
    <rdfs:label xml:lang="de">Mythisches Wesen; Gottheit; Fabelwesen</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdfs:label xml:lang="en">label</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="ExperiencerRole">
    <rdfs:subClassOf rdf:resource="ParticipatorRole"/>
    <dc:description xml:lang="de">Ein/e Wahrnehmende/r ist eine belebte Entität, die eine vom Prädiktat bezeichnete Sinneswahrnehmung oder einen psychischen Zustand erlebt.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">An Experiencer is an animate entity that experiences a sensory perception or mental state designated by the predicate.</dc:description>
    <rdfs:label xml:lang="en">Experiencer Role</rdfs:label>
    <rdfs:label xml:lang="de">Rolle Wahrnehmende/r</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N120013afc15c4c50bf5ffe919e0ed3b9">
    <rdf:rest rdf:nodeID="N388bb763df7e4a50beef849f2954c424"/>
    <rdf:first rdf:resource="Narrative"/>
  </rdf:Description>
  <rdf:Description rdf:about="SemanticRole">
    <dc:description xml:lang="de">Enthält die Informationen einer mit dem Narrativ verbundenen semantischen Rolle.</dc:description>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="en">Contains the information of a semantic role associated with the narrative.</dc:description>
    <rdfs:label xml:lang="de">Semantische Rolle</rdfs:label>
    <rdfs:label xml:lang="en">Semantic Role</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="hasRoWorkURL">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="de">hat Realonline Werk URL</rdfs:label>
    <dc:description xml:lang="de">Verweis auf eine Realonline-Werk-Url.</dc:description>
    <rdfs:domain rdf:resource="VisualRealisation"/>
    <rdfs:label xml:lang="en">has Realonline Work URL</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:subPropertyOf rdf:resource="hasUrl"/>
    <dc:description xml:lang="en">Reference to a realonline work url.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="ThemeRole">
    <rdfs:label xml:lang="de">Rolle Thema</rdfs:label>
    <dc:description xml:lang="en">Theme – The entity that is spatially moved or whose spatial position is indicated by the event (or state) designated in the predicate.</dc:description>
    <rdfs:label xml:lang="en">Theme Role</rdfs:label>
    <rdfs:subClassOf rdf:resource="UndergoerRole"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Thema – Die Entität, die durch das im Prädikat bezeichnete Geschehen (bzw. den bezeichneten Zustand) räumlich bewegt oder räumlich eingeordnet wird.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="toToken">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <dc:description xml:lang="en">This property points from a Text Passage to the last token of this group.</dc:description>
    <rdfs:domain rdf:resource="TextPassage"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <dc:description xml:lang="de">Diese Kante zeigt von einer Textpassage auf das letzte Token aus dieser Gruppe.</dc:description>
    <rdfs:label xml:lang="en">to token</rdfs:label>
    <rdfs:label xml:lang="de">bis Token</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="LocalisationRole">
    <rdfs:subClassOf rdf:resource="SemanticRole"/>
    <dc:description xml:lang="de">Lokaladverbiale; Adverbiale des Raumes.</dc:description>
    <rdfs:label xml:lang="en">Localisation Role</rdfs:label>
    <dc:description xml:lang="en">Local Averbial; Adverbial of Place.</dc:description>
    <rdfs:label xml:lang="de">Rolle Lokalisierung</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N3ef06e3b234d4beb8e54c7c8b4b3f525">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="N1125311ee6ea43dcaec3f35b47b96999"/>
  </rdf:Description>
  <rdf:Description rdf:about="Action">
    <rdfs:label xml:lang="en">Action</rdfs:label>
    <rdfs:label xml:lang="de">Handlung</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Abstrakte Handlungskonzepte aus der Klasse "Verb" werden für jedes Narrativ als eigener Knoten angelegt, welcher der Klasse "Handlung" angehört.</dc:description>
    <dc:description xml:lang="en">Abstract concepts of actions from the class "Verb" are created as a separate node for each narrative, which belongs to the class "Action".</dc:description>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nedd98d27f98947c8810c7fb962d49f9a">
    <rdf:rest rdf:nodeID="Nb277bb4570e54d69aecfc06bca745473"/>
    <rdf:first rdf:resource="Place"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N388bb763df7e4a50beef849f2954c424">
    <rdf:first rdf:resource="PersistentItem"/>
    <rdf:rest rdf:nodeID="Nedd98d27f98947c8810c7fb962d49f9a"/>
  </rdf:Description>
  <rdf:Description rdf:about="PatientRole">
    <rdfs:label xml:lang="de">Rolle Patiens</rdfs:label>
    <dc:description xml:lang="en">Patient - The living entity affected by an event or action and not exercising control over the event; the object or matter affected.</dc:description>
    <rdfs:subClassOf rdf:resource="UndergoerRole"/>
    <rdfs:label xml:lang="en">Patient Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description xml:lang="de">Patiens - Die von einem Geschehen betroffene lebende Entität, die keine Kontrolle auf den Vorgang ausübt; die betroffene Sache oder der betroffene Sachverhalt.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="StimulusRole">
    <rdfs:label xml:lang="de">Rolle Stimulus</rdfs:label>
    <rdfs:label xml:lang="en">Stimulus Role</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="CauserRole"/>
    <dc:description xml:lang="en">The often inanimate trigger of the situation described by the verbal predicate; in combination with the Experiencer Role: the object of a mental state.</dc:description>
    <dc:description xml:lang="de">Der oftmals unbelebte Auslöser der vom Prädikat bezeichneten Situation; in Kombination mit der Rolle Experiencer (Wahrnehmende/r): der Gegenstand eines psychischen Zustands.</dc:description>
  </rdf:Description>
